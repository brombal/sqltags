{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "noEmit": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "useUnknownInCatchVariables": true,
    "types": ["node", "jest"]
  }
}